<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="help_cat_pre_margin">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">6dp</item>
  </style>

  <style name="help_cat">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:paddingTop">2dp</item>
    <item name="android:paddingBottom">2dp</item>
    <item name="android:gravity">center_horizontal</item>
    <item name="android:background">#A4C639</item>
    <item name="android:textColor">#111</item>
    <item name="android:textSize">16sp</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="help_cat_post_margin">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">10dp</item>
  </style>

  <style name="help_pref">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:paddingTop">1dp</item>
    <item name="android:paddingBottom">1dp</item>
    <item name="android:gravity">center_horizontal</item>
    <item name="android:background">#CCC</item>
    <item name="android:textColor">#111</item>
    <item name="android:textSize">14sp</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="help_text">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:paddingTop">7dp</item>
    <item name="android:paddingBottom">7dp</item>
    <item name="android:textSize">14sp</item>
    <item name="android:lineSpacingMultiplier">1.1</item>
  </style>

  <style name="bi_theme" parent="@android:style/Theme.Translucent">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
  </style>

  <color name="light_usage">#A4C639</color>
  <color name="normal_usage">#E8D018</color>
  <color name="constant_usage">#DD6D1D</color>


  <!-- For layout dimensions, fill_parent = -1 and wrap_content = -2 -->

  <array name="alt_theme_default">
    <item>0</item>   <!--main_layout_padding_top_dp-->
    <item>143</item> <!--main_layout_padding_bottom_dp-->
  </array>

  <array name="theme_default">
    <item>-2</item>  <!--main_frame_layout_width-->
    <item>-2</item>  <!--main_frame_layout_height-->
    <item>180</item> <!--main_content_layout_width-->
    <item>-2</item>  <!--main_content_layout_height-->
    <item>7</item>   <!--main_content_padding_left-->
    <item>2</item>   <!--main_content_padding_top-->
    <item>7</item>   <!--main_content_padding_right-->
    <item>7</item>   <!--main_content_padding_bottom-->
    <item>11</item>  <!--title_text_size-->
    <item>8</item>   <!--small_text_size-->
  </array>

  <array name="alt_theme_battery01">
    <item>0</item>   <!--main_layout_padding_top_dp-->
    <item>143</item> <!--main_layout_padding_bottom_dp-->
  </array>

  <array name="theme_battery01">
    <item>-2</item>  <!--main_frame_layout_width-->
    <item>-2</item>  <!--main_frame_layout_height-->
    <item>180</item> <!--main_content_layout_width-->
    <item>-2</item>  <!--main_content_layout_height-->
    <item>7</item>   <!--main_content_padding_left-->
    <item>10</item>  <!--main_content_padding_top-->
    <item>7</item>   <!--main_content_padding_right-->
    <item>7</item>   <!--main_content_padding_bottom-->
    <item>11</item>  <!--title_text_size-->
    <item>8</item>   <!--small_text_size-->
  </array>

  <array name="alt_theme_full_dark">
    <item>0</item>   <!--main_layout_padding_top_dp-->
    <item>0</item>   <!--main_layout_padding_bottom_dp-->
  </array>

  <array name="theme_full_dark">
    <item>-1</item>  <!--main_frame_layout_width-->
    <item>-1</item>  <!--main_frame_layout_height-->
    <item>280</item> <!--main_content_layout_width-->
    <item>-2</item>  <!--main_content_layout_height-->
    <item>7</item>   <!--main_content_padding_left-->
    <item>5</item>   <!--main_content_padding_top-->
    <item>5</item>   <!--main_content_padding_right-->
    <item>5</item>   <!--main_content_padding_bottom-->
    <item>18</item>  <!--title_text_size-->
    <item>10</item>  <!--small_text_size-->
  </array>
</resources>
